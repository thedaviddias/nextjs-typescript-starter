{
  "extends": "@thedaviddias/tsconfig/nextjs.json",
  "compilerOptions": {
    "preserveSymlinks": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "~/*": [
        "./public*"
      ]
    }
  },
  "include": [
    "types/global.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
